@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":AddressBook" as AddressBook MODEL_COLOR
participant ":UniqueEventList" as UniqueEventList MODEL_COLOR
end box

[-> Model : addEvent(toAdd)
activate Model

Model -> AddressBook : addEvent(toAdd)
activate AddressBook


AddressBook -> UniqueEventList : add(toAdd)
activate UniqueEventList
UniqueEventList --> AddressBook
deactivate UniqueEventList

AddressBook --> Model
deactivate AddressBook

[<-- Model
deactivate Model

@enduml
